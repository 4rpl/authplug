{
  "name": "My Extension",
  "description": "My Chrome Extension",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "https://example.com/*",
    "https://google.com/*",
    "https://amazon.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
